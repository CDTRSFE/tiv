{
  "name": "tiv",
  "version": "0.0.5",
  "description": "A Component Library for Vue3.0",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "style": "styles/index.css",
  "typings": "lib/index.d.ts",
  "scripts": {
    "gen": "node ./scripts/gen.js",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:style": "stylelint '{src,website}/**/*.{vue,html,css,less,scss,sass}'",
    "serve": "webpack-dev-server --config ./website/webpack.config.js",
    "build": "rimraf dist && node ./build/version.js && npm run build:com && npm run build:lib && node ./build/copy-files.js",
    "build:com": "node ./build/rollup.config.com.js",
    "build:lib": "rollup --config ./build/rollup.config.lib.js",
    "build:website": "rimraf website/dist && cross-env NODE_ENV=production webpack --config ./website/webpack.config.js",
    "publish:website": "npm run build:website && bash ./scripts/website-publish.sh",
    "publish:lib": "bash ./scripts/publish.sh",
    "gen-log": "git-chglog -o CHANGELOG.md"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "npm run lint",
    "{src,website}/**/*.{vue,html,css,less,scss,sass}": "npm run lint:style"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iamzhaoqing/tiv.git"
  },
  "keywords": [
    "vue"
  ],
  "author": "zhaoqing",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamzhaoqing/tiv/issues"
  },
  "homepage": "https://github.com/iamzhaoqing/tiv#readme",
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@trscd/stylelint-config-tpconfig": "0.0.3",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-standard": "^5.1.2",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "clipboard": "^2.0.6",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-config-tpconfig": "^0.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^7.0.0-0",
    "file-save": "^0.2.0",
    "github-markdown-css": "^4.0.0",
    "hasky": "^3.0.2",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.8",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.5.3",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^7.0.0",
    "markdown-it-container": "^3.0.0",
    "mini-css-extract-plugin": "^1.3.3",
    "rimraf": "^3.0.2",
    "rollup": "^2.36.1",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "transliteration": "^2.2.0",
    "typescript": "^4.1.3",
    "uppercamelcase": "^3.0.0",
    "vue": "^3.0.5",
    "vue-loader": "^16.1.2",
    "vue-router": "^4.0.0-0",
    "webpack": "^4.44.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {},
  "files": [
    "lib",
    "styles",
    "packages"
  ]
}
